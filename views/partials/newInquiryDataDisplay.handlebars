<div class="display-new--holder">
  <div>
    <span class="display-title">Requested Class:</span>
    <span class="display-new--class">Class</span>
  </div>
  <div>
    <span class="display-title">Date Sent:</span>
    <span class="display-new--Date">5/5/2022</span>
  </div>
  <div>
    <span class="display-title">Name:</span>
    <span class="display-new--f-name">Joe</span>
    <span class="display-new--l-name">Johnson</span>
  </div>
  <div>
    <span class="display-title">Email:</span>
    <span class="display-new--email">example@example.com</span>
  </div>
  <div>
    <span class="display-title">Phone Number:</span>
    <span class="display-new--phone">1234567890</span>
    (<span class="text-uppercase display-new--phone-type">cell</span>)
  </div>
  <div>
    <span class="display-title">Participants:</span>
    <span class="display-new--participants">8</span>
  </div>
  <div>
    <span class="display-title">Class Type:</span>
    <span class="display-new--class-type">virtual</span>
  </div>
  <div class="form-check">
    <input
      class="form-check-input"
      type="radio"
      name="dateTimeChoices"
      id="display-new--date-1-radio--SET"
      checked
    />
    <label class="form-check-label display-new--date-1-label" for="display-new--date-1-radio--SET">
      <span class="display-title">Date/Time 1st Choice:</span>
      <span class="display-new--date-1">4/6/2018</span>
      at
      <span class="display-new--time-1">4:00pm</span>
      <span class="display-new--alt-timezone-holder-1">
        (THEIR Date/Time:
        <span class="display-new--local-date-1">4/6/2018</span>
        at
        <span class="display-new--local-time-1">3:00pm</span>)
      </span>
    </label>
  </div>
  <div class="form-check display-new--date-2-holder">
    <input
      class="form-check-input"
      type="radio"
      name="dateTimeChoices"
      id="display-new--date-2-radio--SET"
    />
    <label class="form-check-label display-new--date-2-label" for="display-new--date-2-radio--SET">
      <span class="display-title">Date/Time 2nd Choice:</span>
      <span class="display-new--date-2">4/6/2018</span>
      at
      <span class="display-new--time-2">4:00pm</span>
      <span class="display-new--alt-timezone-holder-2">
        (THEIR Date/Time:
        <span class="display-new--local-date-2">4/6/2018</span>
        at
        <span class="display-new--local-time-2">3:00pm</span>)
      </span>
    </label>
  </div>
  <div class="form-check display-new--date-3-holder">
    <input
      class="form-check-input"
      type="radio"
      name="dateTimeChoices"
      id="display-new--date-3-radio--SET"
    />
    <label class="form-check-label display-new--date-3-label" for="display-new--date-3-radio--SET">
      <span class="display-title">Date/Time 3rd Choice:</span>
      <span class="display-new--date-3">4/6/2018</span>
      at
      <span class="display-new--time-3">4:00pm</span>
      <span class="display-new--alt-timezone-holder-3">
        (THEIR Date/Time:
        <span class="display-new--local-date-3">4/6/2018</span>
        at
        <span class="display-new--local-time-3">3:00pm</span>)
      </span>
    </label>
  </div>
  <div class="form-check">
    {{!  }}
    <input
      class="form-check-input"
      type="radio"
      name="dateTimeChoices"
      id="display-new--date-custom-radio--SET"
      value="requires custom"
    />
    <label
      for="display-new--date-custom-radio--SET"
      class="form-check-label display-new--date-custom-label"
    >
      <span class="display-title">New Date Suggestion</span>
      <input
        class="form-control display-new--custom-time-input"
        type="text"
        placeholder="Alternate Date and Time"
      />
    </label>
  </div>
  <div>
    <span class="display-title">Age Group:</span>
    <span class="display-new--age">Child</span>
  </div>
  <div>
    <span class="display-title">Location:</span>
    <span class="display-new--location">Child</span><span
      class="display-new--location-address"
    ></span>
  </div>
  <div>
    <span class="display-title">Is this class a gift?:</span>
    <span class="display-new--gift">No</span>
  </div>
  <div>
    <span class="display-title">Additional Comments:</span>
    <span class="display-new--comments">hello</span>
  </div>
  <div class="display-title">Response:</div>
  <div class="quillEditor" id="display-new--rich-text--SET">
    {{! set programatically }}
  </div>
  <button type="button" class="btn btn-primary display-new--format-button">Format Response</button>
  <div class="d-none display-new--final-response-holder">
    <div class="giveSpace display-title">Email Subject Line</div>
    <input type="text" class="form-control display-new--subject-line">
    <div class="giveSpace display-title">Formatted Response</div>
    <div class="quillEditor" id="display-new--rich-text--second--SET">
      {{! set programatically }}
    </div>
    <button type="button" class="btn btn-primary display-new--response-button">Send Response</button>
  </div>
</div>