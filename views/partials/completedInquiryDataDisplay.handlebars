<div class="display-completed--holder">
  <div>
    <span class="display-title">Requested Class:</span>
    <span class="display-completed--class">Class</span>
  </div>
  <div>
    <span class="display-title">Date Recieved:</span>
    <span class="display-completed--Date">5/5/2022</span>
  </div>
  <div>
    <span class="display-title">Name:</span>
    <span class="display-completed--f-name">Joe</span>
    <span class="display-completed--l-name">Johnson</span>
  </div>
  <div>
    <span class="display-title">Email:</span>
    <span class="display-completed--email">example@example.com</span>
  </div>
  <div>
    <span class="display-title">Phone Number:</span>
    <span class="display-completed--phone">1234567890</span>
    (<span class="text-uppercase display-completed--phone-type">cell</span>)
  </div>
  <div>
    <span class="display-title">Participants:</span>
    <span class="display-completed--participants">8</span>
  </div>
  <div>
    <span class="display-title">Class Type:</span>
    <span class="display-completed--class-type">virtual</span>
  </div>
  {{!-- date/time --}}
  <div>
    <span class="display-title">Date/Time:</span>
    <span class="display-completed--date-selected">virtual</span>
  </div>
  <div>
    <span class="display-title">Age Group:</span>
    <span class="display-completed--age">Child</span>
  </div>
  <div>
    <span class="display-title">Location:</span>
    <span class="display-completed--location">Child</span>
  </div>
  <div class="d-none display-completed--address-holder">
    <span class="display-title">Address:</span>
    <span class="display-completed--location-address">Child</span>
  </div>
  <div>
    <span class="display-title">Is this class a gift?:</span>
    <span class="display-completed--gift">No</span>
  </div>
  <div>
    <span class="display-title">Additional Comments:</span>
    <span class="display-completed--comments">hello</span>
  </div>
  <div>
    <span class="display-title">Response Email they were sent:</span>
    <p class="display-completed--response-email emailDisplay"></p>
  </div>
  <div class="d-flex p-2">
    <button type="button" class="btn btn-primary display-completed--format-button">Send Thank You Email</button>
    <button class="btn btn-danger display-completed--delete-button-initial giveLeftMargin">Delete Entry</button>
  </div>
  <br>
  <div class="d-none display-completed--delete-confirm">
    <button class="btn btn-lg btn-danger display-completed--delete-button-final">This will PERMANENTLY delete this entry from the database. Click here to confirm.</button>
  </div>
  <div class="d-none display-completed--response-holder">
    <div class="giveSpace display-title">Email Subject Line</div>
    <input type="text" class="form-control display-completed--subject-line">
    <div class="giveSpace display-title">Formatted Response</div>
    <div class="quillEditor" id="display-completed--rich-text--SET">
      {{! set programatically }}
    </div>
    <button type="button" class="btn btn-primary display-completed--response-button">Send Response</button>
  </div>
</div>